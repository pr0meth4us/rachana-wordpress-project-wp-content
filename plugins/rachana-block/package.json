{
	"name": "rachana-block",
	"version": "0.1.0",
	"description": "Example block written with ESNext standard and JSX support â€“ build step required.",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build": "wp-scripts build",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"start": "wp-scripts start",
		"packages-update": "wp-scripts packages-update",
		"build:lib": "rm -rf ./css && webpack --mode=production",
		"compile-sass": "sass blocks/assets/scss/custom/_custom.scss blocks/assets/build/css/styles.css",
		"start:accordion": "wp-scripts start blocks/accordion/src/index.js --output-path=blocks/accordion/build/",
		"build:accordion": "wp-scripts build blocks/accordion/src/index.js --output-path=blocks/accordion/build/",
		"start:hero": "wp-scripts start blocks/hero/src/index.js --output-path=blocks/hero/build/",
		"build:hero": "wp-scripts build blocks/hero/src/index.js --output-path=blocks/hero/build/"
	},
	"dependencies": {
		"@wordpress/block-editor": "^7.0.0",
		"@wordpress/blocks": "^11.0.0",
		"@wordpress/i18n": "^4.2.1",
		"bootstrap": "5.1.3"
	},
	"devDependencies": {
		"@wordpress/scripts": "^17.1.0",
		"css-loader": "^6.7.1",
		"css-minimizer-webpack-plugin": "^3.4.1",
		"mini-css-extract-plugin": "^2.8.0",
		"style-loader": "^3.3.4",
		"webpack": "^5.90.1",
		"webpack-cli": "^5.1.4"
